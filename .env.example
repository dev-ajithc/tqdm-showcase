# TQDM Showcase Environment Variables

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=app.log

# File Operation Settings
TEMP_DIR=temp_files
SAMPLE_DATA_DIR=sample_data

# Download Settings
REQUEST_TIMEOUT=30
MAX_RETRIES=3
