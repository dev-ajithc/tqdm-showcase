# Rate Limiting
REQUEST_DELAY=0.5
MAX_CONCURRENT_REQUESTS=10

# Progress Bar Settings
PROGRESS_BAR_WIDTH=80
PROGRESS_UPDATE_INTERVAL=0.1

# Cache Settings
CACHE_ENABLED=true
CACHE_TTL=86400
